	<div class="row">
		<div class="col-md-12">
			<div class="import">
				<form action="./?action=import" method="post">
					<input type="hidden" name="pre_type" value="keys"/>
					<button type="submit" class="btn blue btn-sm">Importer un fichier CSV</button>
				</form>
			</div>
		</div>
		<div class="col-md-12">
			<!-- BEGIN SAMPLE TABLE PORTLET-->
			<div class="portlet box blue">
				<div class="portlet-title">
					<div class="caption">
						<i class="fa fa-key"></i> Liste des cl√©s  </div>
					<div class="tools">
						<a href="javascript:;" class="collapse" data-original-title="" title=""> </a>
					</div>
				</div>
				<div class="portlet-body">
					<div class="table">
						<table id="borrowingsTable" class="table table-striped table-bordered table-hover">
							<thead>
							<tr>
								<th> ID </th>
								<th> Nom </th>
								<th> Fournisseur </th>
								<th> Type </th>
								<th> Canon </th>
								<th> Nombre d'exemplaires </th>
								<th> Actions </th>
							</tr>
							</thead>
							<tbody>
							{% for key in keys %}
								<tr id="{{ key.id }}">
									<td> {{ key.id }} </td>
									<td> {{ key.name }} </td>
									<td> {{ key.supplier }} </td>
									<td> {% if key.type == "partial" %}partiel {% else %} {{key.type}} {% endif %}</td>
									<td>
										{% if key.locks is iterable %}
											{% for lock in key.locks %}
												{% if lock.id %}
													<p>{{ lock.id }}</p>
												{% else %}
													<p>{{ lock }}</p>
												{% endif %}
											{% endfor %}
										{% endif %}
									</td>
									<td>
										<p
											{% if key.copies < 3 %}
												class="text-danger"
											{% elseif key.copies < 6 %}
												class="text-warning"
											{% endif %}
										>{{ key.copies }}</p>
									</td>

									<td>
										<form action="./?action=updatekey" method="post">
											<input type="hidden" name="update" value="{{ key.id }}"/>
											<button type="submit" class="btn blue btn-sm">Modifier</button>
										</form>

										<!--<form action="" method="post">-->
											<input type="hidden" name="delete" value="{{ key.id }}"/>
											<button type="submit" class="btn red btn-sm btn-delete-k" value="{{ key.id }}">Supprimer</button>
										<!--</form>-->
									</td>
								</tr>
							{% endfor %}
							</tbody>
							<tfoot>
								<th> ID </th>
								<th> Nom </th>
								<th> Fournisseur </th>
								<th> Type </th>
								<th> Canon </th>
								<th> Nombre </th>
								<th> Actions </th>
							</tfoot>
						</table>
					</div>
				</div>
			</div>
			<!-- END SAMPLE TABLE PORTLET-->
		</div>
	</div>
