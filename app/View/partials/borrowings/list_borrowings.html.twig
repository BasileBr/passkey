	<div class="row">
		<div class="col-md-12">
			<!-- BEGIN SAMPLE TABLE PORTLET-->
			<div class="portlet box blue">
				<div class="portlet-title">
					<div class="caption">
						<i class="fa fa-cogs"></i> Liste des emprunts  </div>
					<div class="tools">
						<a href="javascript:;" class="collapse" data-original-title="" title=""> </a>
					</div>
				</div>
				<div class="portlet-body">
					<div class="table-responsive">
						<table class="table table-striped table-bordered table-hover">
							<thead>
							<tr>
								<th> #id </th>
								<th> userEnssatPrimaryKey </th>
								<th> keychainId </th>
								<th> BorrowDate </th>
								<th> dueDate </th>
								<th> returnDate </th>
								<th> lostDate </th>
								<th> comment </th>
							</tr>
							</thead>
							<tbody>
							{% for borrowing in borrowings %}
								<tr>
									<td> {{ borrowing.borrowingId }} </td>
									<td> {{ borrowing.userEnssatPrimaryKey }} </td>
									<td> {{ borrowing.keychainId }} </td>
									<td> {{ borrowing.borrowDate}} </td>
									<td> {{ borrowing.dueDate }} </td>
									<td> {{ borrowing.returnDate }} </td>
									<td> {{ borrowing.lostDate }} </td>
									<td> {{ borrowing.comment }} </td>
								</tr>
							{% endfor %}
							</tbody>
						</table>
					</div>
				</div>
			</div>
			<!-- END SAMPLE TABLE PORTLET-->
		</div>
	</div>
